<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--
	namespace：必须与对应的接口全类名一致
	id:必须与对应接口的某个对应的方法名一致

 -->
<mapper namespace="com.tgb.mapper.Fundmapper">


    <select id="findAll" parameterType="FundInfo">
      SELECT *FROM t_fund
    </select>
    <insert id="saveFund" parameterType="TianTianMyDatasBean">
            insert into t_fund(fund_code,fund_name,fund_new_price,fund_new_rate)  values(#{FCODE},#{SHORTNAME},#{SHORTNAME},#{SHORTNAME},#{SHORTNAME},#{SHORTNAME})
    </insert>

</mapper>